{
  "manifest_version": 3,
  "name": " MAKE^IT easy",
  "version": "1.0",
  "description": "A Chrome extension to take notes and screenshots while studying online.",
  "permissions": [
      "activeTab",
      "storage",
      "scripting"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "icons/icon16.png" }
  }
}

